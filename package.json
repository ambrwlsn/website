{
  "name": "tech-blog-list",
  "version": "1.0.0",
  "description": "Amber's Website",
  "main": "index.html",
  "scripts": {
    "prebuild": "mkdir public && cd public && mkdir css",
    "build:eleventy": "npx @11ty/eleventy --output=public --input=src",
    "concat": "cd src/css && cat variables.css reset.css style.css code.css > ../../public/css/styles.css",
    "delete:public": "rm -r public",
    "start:eleventy": "npx @11ty/eleventy --serve --output=public --input=src",
    "start": "npm-run-all delete:public prebuild concat start:eleventy",
    "build": "npm run build:eleventy",
    "debug": "DEBUG=* eleventy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Amber Wilson",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "2.0.3",
    "date-fns": "^2.16.1",
    "markdown-it": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "npm-run-script": "0.0.4"
  },
  "dependencies": {
    "eleventy-plugin-reading-time": "0.0.1",
    "he": "^1.2.0",
    "striptags": "^3.1.1"
  }
}
